{
  "name": "@bosonprotocol/boson-dcl",
  "version": "2.0.0-alpha.0",
  "description": "Boson Protocol - Decentraland library v2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "docs": "api-documenter markdown --output-folder docs/ --input-folder temp/",
    "build": "rimraf temp/ && rollup -c && npm run docs",
    "lint": "eslint --ignore-path ./.gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "link": "npm link && rollup --watch -c ",
    "test": "jest --detectOpenHandles --silent=false --verbose --colors --runInBand",
    "postinstall": "patch-package",
    "link-cc": "node ./scripts/link-to-local-core-components.js"
  },
  "typings": "./dist/index.d.ts",
  "decentralandLibrary": {
    "main": "./dist/index.js"
  },
  "keywords": [
    "dcl",
    "decentraland",
    "boson protocol",
    "bosonprotocol",
    "library"
  ],
  "author": "-",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/$dcl-author/$dcl-library-name"
  },
  "files": [
    "dist",
    "!dist/types"
  ],
  "devDependencies": {
    "@bosonprotocol/core-sdk": "^1.25.0-alpha.15",
    "@bosonprotocol/eth-connect-sdk": "^1.5.0-alpha.14",
    "@microsoft/api-documenter": "^7.17.11",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "commander": "^10.0.0",
    "decentraland-ecs": "^6.11.4",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eth-connect": "^6.0.3",
    "jest": "^27.5.1",
    "patch-package": "^6.4.7",
    "prettier": "2.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-api-extractor": "^0.2.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "yarn": "please use npm"
  },
  "bundleDependencies": [
    "eth-connect"
  ]
}
